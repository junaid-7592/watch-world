	.carousel-item {
		height: 500px; /* Adjust height as needed */
	}
	
	.carousel-item img {
		object-fit: cover;
		width: 100%;
		height: 100%;
	}
	
	/* Optional: Darken images for better text visibility */
	.carousel-item::after {
		content: '';
		position: absolute;
		top: 0;
		left: 0;
		width: 100%;
		height: 100%;
		background: rgba(0, 0, 0, 0.2);
	}
	
	/* Make carousel controls more visible */
	.carousel-control-prev,
	.carousel-control-next {
		width: 5%;
		opacity: 0.7;
	}
	
	.carousel-control-prev:hover,
	.carousel-control-next:hover {
		opacity: 1;
	}
	
	/* Style indicators */
	.carousel-indicators {
		bottom: 20px;
	}
	
	.carousel-indicators button {
		width: 12px;
		height: 12px;
		border-radius: 50%;
		margin: 0 5px;
	}
